{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "indentWidth": 2,
  "includes": [
    "**/*.{ts,tsx,js,jsx,json}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "out"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.54.0.wasm",
    "https://plugins.dprint.dev/markdown-0.10.0.wasm",
    "https://plugins.dprint.dev/json-0.13.0.wasm"
  ]
}
