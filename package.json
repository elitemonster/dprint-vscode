{
  "name": "dprint",
  "displayName": "Dprint Code Formatter",
  "description": "Formats code in VSCode using dprint.",
  "author": "David Sherret",
  "publisher": "dprint",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dprint/dprint-vscode.git"
  },
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "workspaceContains:**/.dprintrc.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dprint.reset",
        "title": "Dprint: Reset"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.9.7",
    "vscode-test": "^1.4.0"
  }
}